<header>
    <form action="/search" method="GET">
        <input type="text" placeholder="Enter State" name="state">
        <div class="search-service">
            <i class="fas fa-search"></i>
            <input type="text" name="service"
                placeholder="Search your services like income tax consultant, gst consultants and more">
        </div>
        <button type="submit" class="search-icon">
            <i class="fas fa-search"></i>
        </button>

        <div class="nav-links">
            <!-- if user is not logged in (give a check to this) -->
            <a id="loginBtn">Login</a>
            <a id="signupBtn">Signup</a>

            <!-- else if user is logged in -->
            <a id="user-profile" href="/user">Profile</a>
        </div>
    </form>
